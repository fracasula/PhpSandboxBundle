	"type": "symfony-bundle",
	"description": "PHP Sandbox Bundle for Symfony 2",
	"keywords": [
		"Symfony 2",
		"PHP Sandbox",
		"Multitasking"
	],
	"homepage": "http://github.com/fracasula/PhpSandboxBundle",
	"license": "MIT",
	"authors": [
		{
			"name": "Francesco Casula",
			"email": "fra.casula@gmail.com",
			"role": "Developer"
		}
	],
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.4",
		"symfony/filesystem": "~2.7."
	},
	"require-dev": {
		"mikey179/vfsStream": "1.4.*@dev",
		"phpunit/phpunit": "4.5.0"
	},
	"autoload": {
		"psr-0": {
			"FraCasula\\Bundle\\PhpSandboxBundle": ""
		}
